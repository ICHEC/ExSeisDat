digraph G
{
    fontname = "Roboto"
    fontsize = 12
    node
    [
        fontname = "Roboto"
        fontsize = 12
        shape = "record"
    ]
    edge
    [
        fontname = "Roboto"
        fontsize = 12
    ]
    label = "Block level"
    subgraph clusterMPI
    {
        label = "MPI-IO"
        mopen [ label = "{open}" ]
